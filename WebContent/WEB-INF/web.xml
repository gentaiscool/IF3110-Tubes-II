<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>helloJsp.controller.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Registrasi</servlet-name>
    <servlet-class>helloJsp.controller.Registrasi</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Validasi</servlet-name>
    <servlet-class>helloJsp.controller.Validasi</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>helloJsp.controller.Logout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Registrasi</servlet-name>
    <url-pattern>/registrasi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Cart</servlet-name>
    <url-pattern>/cart</url-pattern>
  </servlet-mapping>
</web-app>